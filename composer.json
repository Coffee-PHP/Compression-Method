{
    "name": "coffeephp/compression-method",
    "description": "Provides interfaces for compression method classes to implement.",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Danny Damsky",
            "email": "dannydamsky99@gmail.com"
        }
    ],
    "keywords": [
        "coffeephp",
        "coffee",
        "php",
        "compress",
        "compression",
        "compressor",
        "uncompress",
        "uncompression",
        "uncompressor",
        "decompress",
        "decompression",
        "decompressor",
        "method"
    ],
    "version": "1.1.0",
    "time": "2021-04-23",
    "readme": "README.md",
    "require": {
        "php": ">=8.0",
        "ext-apcu": "*",
        "coffeephp/compress": "1.*",
        "coffeephp/uncompress": "1.*"
    },
    "require-dev": {
        "coffeephp/quality-tools": "1.*",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "CoffeePhp\\CompressionMethod\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "scripts": {
        "coffeephp:exec:phpunit": "coffeephp_exec_phpunit --testsuite unit,integration",
        "coffeephp:exec:psalm": "coffeephp_exec_psalm",
        "coffeephp:exec:publish": "coffeephp_exec_publish"
    }
}
